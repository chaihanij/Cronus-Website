<div class="container">
	<h1 class="page-header">File<p class="lead"></p></h1>
	<div class="blog">
		<div class="row">
			<div class="col-md-9">
				<div class="bs-example">
					<form class="form-horizontal">
					  <div class="form-group">
					    <label class="col-sm-2 control-label" >File name</label>
					    <label class="col-sm-10" style="padding-top: 7px;"><%= @document.documnet_file_name %></label> 
					  </div>
					  <div class="form-group">
					    <label class="col-sm-2 control-label">Url</label>
					    <label class="col-sm-10" style="padding-top: 7px;"><%= link_to "url", @document.documnet.url %></label>    
					  </div>
					  <div class="form-group">
					    <label class="col-sm-2 control-label">Size</label>
					   	<label class="col-sm-10" style="padding-top: 7px;"><%= @document.documnet_file_size %></label>
					  </div>
					  <div class="form-group">
							<label class="col-sm-2 control-label">Upload Time</label>
					   		<label class="col-sm-10" style="padding-top: 7px;"><%= @document.documnet_updated_at %></label>
					  </div>
					</form>
				</div>
		    </div>
	        <div class="col-md-3">          
	        	<%= link_to 'Edit', edit_document_path(@document) %> <br> <%= link_to 'Back', documents_path %>  
	     	</div>
		</div>
		<div class="row">
	        <div class="col-md-4">
				
	     	</div>
		</div>
	</div>
</div>
